{
  "name": "frontend-assignment",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@chakra-ui/core": "^0.8.0",
    "@chakra-ui/react": "^1.7.2",
    "@emotion/react": "11",
    "@emotion/styled": "11",
    "@svgr/webpack": "^5.5.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "framer-motion": "4",
    "next": "^12.0.4",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "dev": "next",
    "start": "next start",
    "lint": "next lint",
    "build": "next build",
    "storybook": "start-storybook -p 6006 -s public",
    "storybook:build": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
